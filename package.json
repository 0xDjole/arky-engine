{
    "name": "tox-server",
    "version": "0.1.0",
    "scripts": {
        "build": "chmod +x build.sh && ./build.sh",
        "deploy": "npm run build && ./deploy.sh",
        "unit-test": "jest --verbose ./__tests__/tests/unit",
        "integration-test": "jest --verbose --silent ./__tests__/tests/integration --maxWorkers=6",
        "e2e-test": "jest --verbose ./__tests__/tests/e2e",
        "lint": "eslint --fix --ext .ts src/** --no-error-on-unmatched-pattern",
        "seed-local": "ts-node src/data --local=true",
        "seed-init": "ts-node src/data --local=true --seedName=init",
        "seed-categories": "ts-node src/data --local=true --seedName=importCategories"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.73",
        "@types/faker": "^5.1.5",
        "@types/jest": "26.0.20",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.168",
        "@types/node": "14.14.20",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "4.13.0",
        "@typescript-eslint/parser": "4.13.0",
        "aws-cdk": "1.95.0",
        "eslint": "^7.17.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-airbnb-typescript": "12.0.0",
        "eslint-config-prettier": "7.1.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jest": "24.1.3",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.3.1",
        "eslint-plugin-react": "7.22.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "jest": "26.6.3",
        "prettier": "2.2.1",
        "ts-jest": "26.4.4",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
    },
    "dependencies": {
        "@aws-cdk/assert": "1.95.0",
        "@aws-cdk/aws-appsync": "1.95.0",
        "@aws-cdk/aws-autoscaling": "1.95.0",
        "@aws-cdk/aws-codebuild": "1.95.0",
        "@aws-cdk/aws-codepipeline": "1.95.0",
        "@aws-cdk/aws-codepipeline-actions": "1.95.0",
        "@aws-cdk/aws-iam": "1.95.0",
        "@aws-cdk/aws-lambda": "1.95.0",
        "@aws-cdk/aws-lambda-event-sources": "1.95.0",
        "@aws-cdk/aws-s3": "1.95.0",
        "@aws-cdk/aws-s3-deployment": "1.95.0",
        "@aws-cdk/aws-secretsmanager": "1.95.0",
        "@aws-cdk/aws-ssm": "1.95.0",
        "@aws-cdk/core": "1.95.0",
        "@aws-cdk/pipelines": "1.95.0",
        "@elastic/elasticsearch": "7.10.0",
        "apiCall": "file:src/apiCall",
        "appSyncApi": "file:src/appSyncApi",
        "aws-lambda": "^1.0.6",
        "aws-sdk": "2.825.0",
        "axios": "0.21.1",
        "cassandra-driver": "4.6.3",
        "config": "file:src/config",
        "crypto": "1.0.1",
        "dotenv": "8.2.0",
        "envfile": "6.14.0",
